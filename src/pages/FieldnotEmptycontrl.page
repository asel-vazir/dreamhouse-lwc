<apex:page controller="FieldnotEmptycontrl">
        <apex:form >
            <apex:pageBlock >
                <apex:pageBlockSection columns="1">

                    <apex:inputField value="{!field.Business__c}" required="true"/>

                    <apex:inputField value="{!field.Contact__c}" required="true"/>

                </apex:pageBlockSection>
